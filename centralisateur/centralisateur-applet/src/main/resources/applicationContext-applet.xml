<?xml version="1.0" encoding="ISO-8859-1"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans.xsd">
      
    <bean name="remoteVpnService" class="org.springframework.remoting.httpinvoker.HttpInvokerProxyFactoryBean">
        <property name="serviceUrl" value="http://localhost:8080/centralisateur-war/remoting/AppletService"/>
        <property name="serviceInterface" value="jkt.centralisateur.applet.remoting.RemoteVpnService"/>
    </bean>
      
</beans>
